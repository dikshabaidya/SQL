CASE
   WHEN ts.itype = 2 THEN 'Checks'
   WHEN ts.itype = 3 THEN 'Invoices'
   WHEN ts.itype = 4 THEN 'Recurring Invoices'
   WHEN ts.itype = 5 THEN 'Bank Deposit'
   WHEN ts.itype = 6 THEN 'Receipt'
   WHEN ts.itype = 7 THEN 'Charge'
   WHEN ts.itype = 8 THEN 'Recurring Charge'
   WHEN ts.itype = 9 THEN 'Check Batch (International Only)'
   WHEN ts.itype = 10 THEN 'Journal Entry'
   WHEN ts.itype = 11 THEN 'Batch Invoice'
   WHEN ts.itype = 12 THEN 'Batch Receipt'
   WHEN ts.itype = 13 THEN 'Batch Journal Entry'
   WHEN ts.itype = 15 THEN 'Transaction in a Batch'
   WHEN ts.itype = 16 THEN 'Batch Charge'
   WHEN ts.itype = 18 THEN 'Bank Adjustment'
   WHEN ts.itype = 20 THEN 'Recurring Journal Entry'
END AS 'Transaction Type'
